<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/corona"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid19Tracker">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".EachCountryDataActivity" />
        <activity android:name=".CountrywiseDataActivity" />
        <activity android:name=".EachStateDataActivity" />
        <activity android:name=".StatewiseDataActivity" />
        <activity android:name=".WorldDataActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>